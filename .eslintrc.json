{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"standard-with-typescript",
		"plugin:react/recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"ignorePatterns": ["src/reportWebVitals.js", "src/setupProxy.js"],
	"rules": {
		"@typescript-eslint/explicit-function-return-type": "off"
	}
}
